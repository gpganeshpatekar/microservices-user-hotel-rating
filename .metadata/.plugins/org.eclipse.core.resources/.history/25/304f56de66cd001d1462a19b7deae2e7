server:
  port: 8081

spring:
  datasource:
    url: jdbc:mysql://localhost:3306/m-user-service
    username: root
    password: root
    dbcp2:
      driver-class-name: com.mysql.cj.jdbc.Driver
 
  jpa:
    hibernate:
      ddl-auto: update
    properties:
      hibernate:
        dialect: hibernate.dialect.MySQL8Dialect